{{#if colors.length}}
  {{#each computed_colors as |row|}}
    <div class='extra_color_row_preview'>
      <div class='box' style={{row.style}}>
      </div>
      {{row.label}}
    </div>
  {{/each}}
  <br/>
{{else}}
  {{t "None Set" key='none_set'}}
{{/if}}
<button class='btn btn-default' {{action 'modify'}} style='margin-left: 30px;'>{{t "Modify" key='modify'}}</button>
